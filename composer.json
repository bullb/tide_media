{
    "name": "dpc-sdp/tide_media",
    "description": "Media and related configuration for Tide Drupal 8 distribution",
    "type": "drupal-module",
    "license": "GPL-2.0-or-later",
    "require": {
        "dpc-sdp/tide_core": "^1.2.6",
        "drupal/embed": "^1.0",
        "drupal/inline_entity_form": "^1.0-rc1",
        "drupal/svg_image": "^1.8",
        "drupal/video_embed_field": "^2.0"
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "extra": {
        "enable_patching": true,
        "patches": {
            "dpc-sdp/tide_core": {
                "Added version constraint for entity_reference_revisions - https://github.com/dpc-sdp/tide_core/pull/107": "https://patch-diff.githubusercontent.com/raw/dpc-sdp/tide_core/pull/107.diff"
            }
        }
    }
}
